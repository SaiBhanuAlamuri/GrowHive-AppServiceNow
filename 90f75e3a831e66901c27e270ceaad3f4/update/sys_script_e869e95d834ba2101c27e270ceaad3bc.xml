        <script><![CDATA[(function executeRule(current, previous, isUpdate) {
    var source = gs.getSession().getClientTransaction().source;
    var actualStatus = current.status + '';

    gs.info("BR Triggered | Source: " + source + " | Status: " + actualStatus);

    // Match exactly with DB value ("Closed" with capital C)
    if (actualStatus.trim() == 'Closed') {
        if (source != 'rest') {
            gs.addErrorMessage("You cannot manually set status to Closed.");
            current.setAbortAction(true);
        }
    }
})();
